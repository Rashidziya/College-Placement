#include<bits/stdc++.h>
#include<string>
using namespace std;

// Remvoe Vowels from the string 

int main(){
    string str = "prepinsta";
    
    for(int i = 0; i<str.length(); i++){
        char ch = str[i];
        if(ch == 'A' || ch == 'E' || ch == 'I' || ch=='O' || ch == 'U' ||
        ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u'){
            str.erase(str.begin() + i);
        }
    }
    
    cout<<str<<endl;
    return 0;
}
